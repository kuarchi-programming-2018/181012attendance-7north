   "source": [
    "def fib(n):\n",
    "    a, b = 0, 1\n",
    "    for i in range(n):\n",
    "        a, b = b, a + b\n",
    "    return b\n",
    "\n",
    "#print([fib(i) for i in range(num)])  #n =整数\n",
    "num=int(input())\n",
    "\n",
    "i = 0\n",
    "while True:\n",
    "    if fib(i) >= num: break\n",
    "    i += 1\n",
    "\n",
    "print(\"n=\" +str(num))\n",
    "print(\"n以下で最大のフィボナッチ数 argfib(n)=\" +str(fib(i-1)))\n"
   ]
